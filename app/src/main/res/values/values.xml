<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">RubberPicker</string>
    <declare-styleable name="RubberRangePicker">
        <attr name="stretchRange" />
        <attr name="elasticBehavior" />
        <attr name="thumbDrawable" />
        <attr name="defaultThumbRadius" />
        <attr name="normalTrackWidth" />
        <attr name="highlightTrackWidth" />
        <attr name="normalTrackColor" />
        <attr name="highlightTrackColor" />
        <attr name="highlightDefaultThumbOnTouchColor" />
        <attr name="defaultThumbInsideColor" />
        <attr name="dampingRatio" />
        <attr name="stiffness" />
        <attr name="minValue" />
        <attr name="maxValue" />
        <attr name="initialStartValue" format="integer" />
        <attr name="initialEndValue" format="integer" />
    </declare-styleable>
    <declare-styleable name="RubberSeekBar">
        <attr name="stretchRange" format="dimension" />
        <attr name="elasticBehavior" format="enum">
            <enum name="linear" value="0" />
            <enum name="cubic" value="1" />
            <enum name="rigid" value="2" />
        </attr>
        <attr name="thumbDrawable" format="reference" />
        <attr name="defaultThumbRadius" format="dimension" />
        <attr name="normalTrackWidth" format="dimension" />
        <attr name="highlightTrackWidth" format="dimension" />
        <attr name="normalTrackColor" format="color" />
        <attr name="highlightTrackColor" format="color" />
        <attr name="highlightDefaultThumbOnTouchColor" format="color" />
        <attr name="defaultThumbInsideColor" format="color" />
        <attr name="dampingRatio" format="float" />
        <attr name="stiffness" format="float" />
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="initialValue" format="integer" />
    </declare-styleable>
</resources>